  "bugs": {
    "url": "https://github.com/jandiralceu/rca/issues",
    "email": "me@jandir.co"
  },
  "homepage": "https://rca.github.com",
  "scripts": {
    "develop": "webpack serve",
    "lint:eslint": "node_modules/.bin/eslint --ext .ts,.tsx --fix ./src",
    "lint:prettier": "prettier --write ./src",
    "lint:check": "prettier --check ./src",
    "build": "webpack",
    "stats": "webpack --json build-stats.json",
    "prepare": "husky install",
    "test": "jest --bail --passWithNoTests --no-cache --runInBand",
    "test:watch": "yarn test --watch",
    "test:staged": "yarn test --findRelatedTests",
    "test:ci": "yarn test --coverage",
    "test:e2e": "cypress open"
  },
